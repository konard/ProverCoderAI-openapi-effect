{
	"threshold": 0,
	"minTokens": 30,
	"minLines": 5,
	"ignore": [
		"**/node_modules/**",
		"**/build/**",
		"**/dist/**",
		"**/*.min.js",
		"**/reports/**",
		"**/generated/**",
		"**/fixtures/**",
		"**/tests/api-client/**",
		"**/src/shell/api-client/create-client.ts",
		"**/src/index.ts",
		"**/openapi.d.ts"
	],
	"skipComments": true,
	"ignorePattern": [
		"private readonly \\w+: \\w+;\\s*private readonly \\w+: \\w+;\\s*private \\w+: \\w+ \\| null = null;\\s*private \\w+: \\w+ \\| null = null;"
	]
}
